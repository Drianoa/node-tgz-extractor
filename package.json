{
  "name": "node-tgz-extractor",
  "description": "Extracts all of the node_modules from npm cache based on a package-lock.json file",
  "authors": "Drianoa",
  "version": "1.0.0-alpha",
  "scripts": {
    "test": "jasmine test/**/*spec.js --reporter=jasmine-console-reporter"
  },
  "keywords": [
    "tgz",
    "npm",
    "package.json",
    "package-lock",
    "registry"
  ],
  "dependencies": {
    "cacache": "^16.1.3",
    "colors": "^1.3.0",
    "commander": "^2.16.0",
    "https-proxy-agent": "^5.0.1",
    "mkdirp": "^1.0.4",
    "p-limit": "^4.0.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "semver": "^5.5.0",
    "tar": "^4.4.6"
  },
  "main": "lib/extractor.js",
  "bin": {
    "download-tgz": "bin/extract-tgz"
  },
  "files": [
    "lib",
    "bin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Driaona/node-tgz-extractor.git"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.9",
    "jasmine": "^3.2.0",
    "jasmine-console-reporter": "^3.1.0",
    "rimraf": "^2.6.2"
  }
}
